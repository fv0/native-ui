{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true,
    "node": true,
    "mocha": true
  },
  "globals": {
      "atom"
  },
  "rules": {
    "comma-style": [
      2,
      "last"
    ],
    "no-debugger": 0,
    "strict": 0,
    "semi": ["error", "always"],
    "linebreak-style": 0, // 0 = off, 1 = warn, 2 = error
    "no-bitwise": 2,  //
    "no-cond-assign": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-unused-expressions": 1,
    "guard-for-in": 1,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-use-before-define": 2,
    "no-loop-func": 1,
    "no-caller": 2,
    "no-script-url": 2,
    "no-shadow": 2,
    "no-new-func": 0,
    "no-new-wrappers": 0,
    "no-undef": 2,
    "no-unused-vars": 1,
    "new-cap": 1,
    "no-empty": 2,
    "no-new": 2,
    "no-plusplus": 0,
    "dot-notation": 0
  }
}
